{
  "sortedGroupName": "sorted_videos",
  "dataDir": "data",
  "sessionFile": "data/telegram_session.session",
  "videoMatches": [
    "keyword1",
    "keyword2",
    "keyword3"
  ],
  "videoExclusions": [
    "compilation",
    "preview"
  ],
  "minVideoDurationInSeconds": 200,
  "maxVideoDurationInSeconds": 360000,
  "minFileSizeMB": 10,
  "maxFileSizeMB": 2000,
  "maxForwards": 5000,
  "dryRun": false,
  "skipCleanup": false,
  "sourceGroups": [],
  "duplicateDetection": {
    "checkDuration": true,
    "durationToleranceSeconds": 30,
    "checkFileSize": true,
    "fileSizeTolerancePercent": 5,
    "normalizeFilenames": true,
    "checkResolution": false,
    "resolutionTolerancePercent": 10,
    "checkMimeType": false
  }
}
